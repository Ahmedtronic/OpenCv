    "import cv2\n",
    "cam = cv2.VideoCapture(0)\n",
    "h = cam.get(cv2.CAP_PROP_FRAME_HEIGHT)\n",
    "w = cam.get(cv2.CAP_PROP_FRAME_WIDTH)\n",
    "print (h,w)\n",
    "\n",
    "\n",
    "while cam.isOpened():\n",
    "    err,img = cam.read()\n",
    "    cv2.imshow(\"lalala\", img)\n",
    "    k = cv2.waitKey(10) & 0xff\n",
    "    if k==27:\n",
    "        break\n"
